#include<stdio.h>
int main()
{
	int t,n,k,i,j;
	scanf("%d",&t);
	while(t>0)
	{
		 scanf("%d",&n);
			k = n-(n/2);
			for(i = 0 ; i <  n ;i++)
			{
				for(j= 0  ; j < n ;j++)
				{
					printf("%d ",k);
					k++;
					if(j+1==n)
						k--;
					if(k==n+1)
						k=1;
				}
				printf("\n");
			}
			t--;
	}
	
}
